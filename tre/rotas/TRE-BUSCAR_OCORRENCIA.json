{
  "tabela": {
    "nome": "PCINTEGRACAOROTASERVICO",
    "campos": [
      {
        "nome": "ID",
        "valor": "TRE-BUSCAR_OCORRENCIA"
      },
      {
        "nome": "IDEMPRESAAPI",
        "valor": "TRE"
      },
      {
        "nome": "SERVICO",
        "valor": "TRE-BUSCAR_OCORRENCIA"
      },
      {
        "nome": "LAYOUTCOMUNICACAO",
        "valor": {
          "urlWsdl": "https://winthorqa.cloudneolog.com.br/cockpit-gateway/monitoring-integration-publishing-soap/ws/retrieveOccurrence",
          "header": [
            {
              "key": "userToken",
              "value": "{{TOKEN}}"
            }
          ],
          "operacao": {
            "request": ""
          }
        }
      },
      {
        "nome": "LAYOUTTRANSFORMACAO",
        "valor": [
          {
            "operation": "shift",
            "spec": {
              "SOAP-ENV:Body": {
                "ns2:retrieveResponse": {
                  "ns2:occurrenceSet": {
                    "*": {
                      "ns2:occurrences": {
                        "ns2:occurrence": {
                          "ns2:monitorableSourceId": "temp[\u00263].identificador",
                          "ns2:monitorableType": "temp[\u00263].tipoMonitoravel",
                          "ns2:creationDate": "temp[\u00263].dataCriacao",
                          "ns2:status": "temp[\u00263].tipoRecado",
                          "ns2:cause": {
                            "ns2:name": "temp[\u00264].causa"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "operation": "shift",
            "spec": {
              "temp": {
                "*": "valores[]"
              }
            }
          },
          {
            "operation": "modify-overwrite-beta",
            "spec": {
              "valores": {
                "*": {
                  "idExterno": "\u003dconcat(\u0027SOAP-TRE-OCORRENCIA-\u0027,@(1,tipoMonitoravel),\u0027-\u0027,@(1,identificador), \u0027-\u0027,@(1,dataCriacao))",
                  "idInterno": "\u003dconcat(\u0027\u0027, @(1,identificador))",
                  "tipoIdInterno": "TRE-OCORRENCIA"
                }
              }
            }
          },
          {
            "operation": "shift",
            "spec": {
              "valores": {
                "*": {
                  "idExterno": "idExterno[]",
                  "idInterno": "idInterno",
                  "tipoIdInterno": "tipoIdInterno",
                  "identificador": "data.[\u00261].identificador",
                  "tipoMonitoravel": "data.[\u00261].tipoMonitoravel",
                  "tipoRecado": "data.[\u00261].tipoRecado",
                  "causa": "data.[\u00261].causa"
                }
              }
            }
          },
          {
            "operation": "default",
            "spec": {
              "_attr_access": "data"
            }
          }
        ]
      },
      {

            "spec": {
              "items": {
                "*": {
                  "idExterno": "\u003dconcat(\u0027SOAP-TRE-NOTAS\u0027,@(1,numero),\u0027-\u0027,@(1,situacao))",
                  "idInterno": "\u003dconcat(\u0027\u0027, @(1,numero))",

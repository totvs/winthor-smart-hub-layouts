                  }
                }
              }
            }
          },
          {
            "operation": "modify-overwrite-beta",
            "spec": {
              "items": {
                "*": {
                  "peso": "\u003dtoInteger",
                  "valor": "\u003dtoInteger"
                }
              }
            }
          },
          {
            "operation": "shift",
            "spec": {
              "items": {
                "*": {
                  "idExterno": "idExterno[]",
                  "idInterno": "idInterno",
                  "tipoIdInterno": "tipoIdInterno",
                  "numero": "data.[\u00261].soapenv:Body.tns:deleteInvoices.tns:invoices.tns:invoice.tns:identifier",
                  "peso": "data.[\u00261].soapenv:Body.tns:deleteInvoices.tns:invoices.tns:invoice.tns:totalWeight",
                  "valor": "data.[\u00261].soapenv:Body.tns:deleteInvoices.tns:invoices.tns:invoice.tns:totalValue",
                  "cliente": "data.[\u00261].soapenv:Body.tns:deleteInvoices.tns:invoices.tns:invoice.tns:name",
                  "chave": {
                    "*": {
                      "$": "data.[\u00263].soapenv:Body.tns:deleteInvoices.tns:invoices.tns:invoice.tns:NFeKey"
                    }
                  },
                  "pedido": {
                    "*": "data.[\u00262].soapenv:Body.tns:deleteInvoices.tns:invoices.tns:invoice.tns:orderSourceIds.tns:string[\u0026]"
                  },
                  "#{{REGION_SOURCE_ID}}": "data.[\u00261].soapenv:Body.tns:deleteInvoices.tns:invoices.tns:invoice.tns:regionSourceId",
                  "#http://schemas.xmlsoap.org/soap/envelope/": "data.[\u00261].-xmlns:soapenv",
                  "#http://www.neolog.com.br/cpl/acquisition/invoice/": "data.[\u00261].-xmlns:tns"
                }
              }
            }

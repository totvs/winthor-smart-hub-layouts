      },
      {
        "nome": "SERVICO",
        "valor": "TRE-BUSCAR_TRANSICAO"
      },
      {
        "nome": "LAYOUTCOMUNICACAO",
        "valor": {
          "urlWsdl": "https://winthorqa.cloudneolog.com.br/cockpit-gateway/monitoring-integration-publishing-soap/ws/retrieveTransition",
          "header": [
            {
              "key": "userToken",
              "value": "{{TOKEN}}"
            }
          ],
          "operacao": {
            "request": ""
          }
        }
      },
      {
        "nome": "LAYOUTTRANSFORMACAO",
        "valor": [
          {
            "operation": "shift",
            "spec": {
              "SOAP-ENV:Body": {
                "ns2:retrieveResponse": {
                  "ns2:messages": {
                    "ns2:transitionSet": {
                      "*": {
                        "ns2:transitions": {
                          "ns2:transition": {
                            "ns2:monitorableSourceId": "temp[\u00263].identificador",
                            "ns2:monitorableType": "temp[\u00263].tipoMonitoravel",
                            "ns2:status": "temp[\u00263].tipoRecado",
                            "ns2:executedTimestamp": "temp[\u00263].dataExecutada",
                            "ns2:name": "temp[\u00263].causa"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "operation": "modify-overwrite-beta",
            "spec": {
              "temp": {
                "*": {
                  "idExterno": "\u003dconcat(\u0027SOAP-TRE-TRANSICAO-\u0027,@(1,tipoMonitoravel),\u0027-\u0027,@(1,identificador), \u0027-\u0027,@(1,dataExecutada))",
                  "idInterno": "\u003dconcat(\u0027\u0027, @(1,identificador))",
                  "tipoIdInterno": "TRE-TRANSICAO"
                }
              }
            }
          },
          {
            "operation": "shift",
            "spec": {
              "temp": {
                "*": {
                  "idExterno": "idExterno[]",
                  "idInterno": "idInterno[]",
                  "tipoIdInterno": "tipoIdInterno[]",
                  "identificador": "data[\u00261].identificador",
                  "tipoMonitoravel": "data[\u00261].tipoMonitoravel",
                  "tipoRecado": "data[\u00261].tipoRecado",
                  "causa": "data[\u00261].causa"
                }
              }
            }
          },
          {
            "operation": "default",
            "spec": {
              "_attr_access": "data"
            }
          }
        ]
      },

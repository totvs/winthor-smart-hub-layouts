          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "*/*"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{TOKEN}}"
              }
            ],
            "url": {
              "raw": "{{URL_BASE}}/logistica/apis/v1/localidades/buscar",
              "query": [
                {
                  "key": "page",
                  "value": "{{PAGE}}"
                },
                {
                  "key": "pageSize",

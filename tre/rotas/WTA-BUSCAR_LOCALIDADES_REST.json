                }
              ],
              "host": [
                "{{URL_BASE}}"
              ],
              "path": [
                "logistica",
                "apis",
                "v1",
                "localidades",
                "buscar"
              ]
            }
          },
          "response": []
        }
      },
      {
        "nome": "LAYOUTTRANSFORMACAO",
        "valor": [
          {
            "operation": "modify-overwrite-beta",
            "spec": {
              "items": {
                "*": {
                  "idExterno": "=concat('SOAP-TRE-',@(1,idLocalidade),'-',@(1,dataUltimaAlteracao))",
                  "idInterno": "=concat('', @(1,idLocalidade))",
                  "tipoIdInterno": "TRE-LOCALIDADE"
                }
              }
            }
          },
          {
            "operation": "modify-overwrite-beta",
            "spec": {
              "items": {
                "*": {
                  "endereco": "=concat(@(1,endereco), ' ', @(1,numero), ' - ', @(1,bairro), ' ', @(1,municipio), ' - ', @(1,estado), ' ', @(1,cep))"
                }
              }
            }
          },
          {
            "operation": "shift",
            "spec": {
              "items": {
                "*": {
                  "idExterno": "idExterno[]",
                  "idInterno": "idInterno",
                  "tipoIdInterno": "tipoIdInterno",
                  "nome": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:name",
                  "numero": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:addressNumber",
                  "descricao": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:description",
                  "municipio": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:addressCity",
                  "estado": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:addressStateAcronym",
                  "bairro": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:addressDistrict",
                  "endereco": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:addressStreet",
                  "cep": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:addressZipCode",
                  "siglaPais": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:addressCountryAcronym",
                  "idLocalidade": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:identifier",
                  "tipoEndereco": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:logisticRoleId",
                  "#2002": "data.[&1].soapenv:Body.tns:createLocalities.tns:localities.tns:locality.tns:regionSourceId",
                  "#http://schemas.xmlsoap.org/soap/envelope/": "data.[&1].-xmlns:soapenv",
                  "#http://www.neolog.com.br/cpl/acquisition/locality/": "data.[&1].-xmlns:tns"
                }
              }
            }
          },
          {
            "operation": "default",
            "spec": {
              "_attr_access": "data"
            }
          }
        ]
      },
      {
        "nome": "ATIVO",
        "valor": "N"

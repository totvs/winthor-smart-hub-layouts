                  "publishReleasedTrips": {
                    "publishReleasedTrip": {
                      "messageId": "idMensagem",
                      "tripReleaseRequests": {
                        "tripReleaseRequest": {
                          "*": {
                            "identifier": "items.[\u00261].idViagem",
                            "loads": {

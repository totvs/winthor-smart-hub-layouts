{
    "tabela": {
        "nome": "PCINTEGRACAOROTASERVICO",
        "campos": [
            {
                "nome": "DATASINCRONISMO",
                "valor": ""
            },
            {
                "nome": "REFRESHTOKEN",
                "valor": "N"
            },
            {
                "nome": "SOMENTEATUALIZARINTEGRACAOCORE",
                "valor": "N"
            },
            {
                "nome": "ID",
                "valor": "COLENT - Buscar Entrega"
            },
            {
                "nome": "IDEMPRESAAPI",
                "valor": "COLETA-ENTREGA"
            },
            {
                "nome": "SERVICO",
                "valor": "COLENT - Buscar Entrega"
            },
            {
                "nome": "LAYOUTCOMUNICACAO",
                "valor": {
                    "name": "COLENT - Buscar Entrega",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{TOKEN}}"
                            },
                            {
                                "key": "Accept",
                                "value": " */*"
                            },
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "url": {
                            "raw": "https://winthor-log.logistica.totvs.app/coletaentrega/query/api/v1/coletasEntregas",
                            "query": [
                                {
                                    "key": "page",
                                    "value": "{{PAGE}}"
                                },
                                {
                                    "key": "pageSize",
                                    "value": "{{PAGE_SIZE}}"
                                },
                                {
                                    "key": "tipo",
                                    "value": "ENTREGA"
                                },
                                {
                                    "key": "externalId",
                                    "value": "{{NUMTRANSVENDA}}"
                                }
                            ]
                        }
                    },
                    "response": []
                }
            },
            {
                "nome": "LAYOUTTRANSFORMACAO",
                "valor": [
                    {
                        "operation": "modify-overwrite-beta",
                        "spec": {
                            "items": {
                                "*": {
                                    "idExterno": "\u003dconcat(\u0027coleta-entrega-busca-entrega-\u0027,@(1,id), \u0027\u0027, @(1,dataSituacao))"
                                }
                            }
                        }
                    },
                    {
                        "operation": "shift",
                        "spec": {
                            "items": {
                                "*": {
                                    "idExterno": "idExterno",
                                    "externalId": "items.[\u00261].numTransVenda",
                                    "id": "items.[\u00261].idExterno",
                                    "dataSituacao": "items.[\u00261].dataSituacao"
                                }
                            }
                        }
                    },
                    {
                        "operation": "default",
                        "spec": {
                            "_attr_access": "items"
                        }
                    }
                ]
            },
            {
                "nome": "ATIVO",
                "valor": "S"
            },
            {
                "nome": "TIPOPROCESSO",
                "valor": ""
            },
            {
                "nome": "AUTENTICADOR",
                "valor": "N"
            }
        ]
    }

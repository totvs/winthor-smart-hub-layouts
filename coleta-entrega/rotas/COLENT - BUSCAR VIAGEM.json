            },
            {
                "nome": "LAYOUTCOMUNICACAO",
                "valor": {
                    "name": "COLENT - Buscar Viagem",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{TOKEN}}"
                            },
                            {
                                "key": "Accept",
                                "value": " */*"
                            },
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "url": {
                            "raw": "{{URL_BASE}}/query/api/v1/viagens",
                            "query": [
                                {
                                    "key": "externalIdViagem",
                                    "value": "{{NUMCAR}}"
                                }
                            ],
                            "host": [
                                "{{URL_BASE}}"
                            ]
                        }
                    },
                    "response": []
                }
            },
            {
                "nome": "LAYOUTTRANSFORMACAO",
                "valor": [
                    {
                        "operation": "shift",
                        "spec": {
                            "items": {
                                "*": {
                                    "externalId": "items.[\u00261].numcar",
                                    "id": [
                                        "items.[\u00261].idExterno",
                                        "idExterno"
                                    ],
                                    "dataSituacao": "items.[\u00261].dataSituacao"
                                }
                            }
                        }
                    },
                    {
                        "operation": "default",
                        "spec": {
                            "_attr_access": "items"
                        }
                    }
                ]
            },
            {
                "nome": "ATIVO",

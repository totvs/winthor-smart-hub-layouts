                                    "codCob": "=join('',@(1,codCob_arr))",
                                    "codPlPag_arr": "=split('//.',@(1,conteudo.VendaPagamentos.[0].PagamentoCondicao.IdRetaguarda))",
                                    "codPlPag": "=join('',@(1,codPlPag_arr))",

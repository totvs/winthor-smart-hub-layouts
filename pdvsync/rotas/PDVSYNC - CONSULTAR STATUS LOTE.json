{
	"tabela": {
		"nome": "PCINTEGRACAOROTASERVICO",
		"campos": [
			{
				"nome": "SOMENTEATUALIZARINTEGRACAOCORE",
				"valor": "N"
			},
			{
				"nome": "ID",
				"valor": "PDVSYNC - Consultar status lote"
			},
			{
				"nome": "IDEMPRESAAPI",
				"valor": "PDVSYNC"
			},
			{
				"nome": "SERVICO",
				"valor": "PDVSYNC - Consultar status lote"
			},
			{
				"nome": "LAYOUTCOMUNICACAO",
				"valor": {
					"name": "PDVSYNC - Consultar status lote",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{TOKEN}}"
							},
							{
								"key": "Accept",
								"value": "*/*"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"url": {
							"raw": "{{BASE_URL_STATUS_LOTE}}/{{ID_INQUILINO}}/{{NUMERO_LOTE}}",
							"query": []
						}
					},
					"response": []
				}
			},
			{
				"nome": "LAYOUTTRANSFORMACAO",
				"valor": [
					{
						"operation": "shift",
						"spec": {
							"data": {
								"*": {
									"loteOrigem": [
										"items[&1].numeroLote",
										"items[&1].idExterno"
									],
									"status": {
										"2": {
											"#11": "items[&3].status"
										},
										"2.0": {
											"#11": "items[&3].status"
										},
										"4": {
											"#11": "items[&3].status"
										},
										"4.0": {
											"#11": "items[&3].status"
										},
										"5": {
											"#12": "items[&3].status"
										},
										"5.0": {
											"#12": "items[&3].status"
										},
										"6": {
											"#13": "items[&3].status"
										},
										"6.0": {
											"#13": "items[&3].status"
										},
										"*": {
											"#99": "items[&3].origem"
										}
									},
									"errosIdentificados": {
										"*": {
											"idRetaguarda": "items[&3].erros[&1].idInterno",
											"erro": "items[&3].erros[&1].erro"
										}
									}
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"idExterno": "=concat('pdvsync-post-status-', @(1,status),'-lote-', @(1,idExterno))"
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"numeroLote": "items[&1].numeroLote",
									"idExterno": "idExterno",
									"status": "items[&1].status",
									"erros": "items[&1].erros"
								}
							}
						}
					},
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"status": [
										"=toInteger",
										0
									]
								}
							}
						}
					},
					{
						"operation": "default",
						"spec": {
							"_attr_access": "items"
						}
					}
				]
			},
			{
				"nome": "ATIVO",
				"valor": "S"
			},
			{
				"nome": "AUTENTICADOR",
				"valor": "N"
			},
			{
				"nome": "DATASINCRONISMO",
				"valor": "17-MAR-23"
			},
			{
				"nome": "REFRESHTOKEN",
				"valor": ""
			}
		]
	}
}
{
	"tabela": {
		"nome": "PCINTEGRACAOROTASERVICO",
		"campos": [
			{
				"nome": "SOMENTEATUALIZARINTEGRACAOCORE",
				"valor": "N"
			},
			{
				"nome": "ID",
				"valor": "WTA - Buscar lote PDV"
			},
			{
				"nome": "IDEMPRESAAPI",
				"valor": "WINTHOR-WTA"
			},
			{
				"nome": "SERVICO",
				"valor": "WTA - Buscar lote PDV"
			},
			{
				"nome": "LAYOUTCOMUNICACAO",
				"valor": {
					"name": "WTA - Buscar Lote PDV",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{TOKEN}}"
							},
							{
								"key": "Accept",
								"value": "*/*"
							}
						],
						"url": {
							"raw": "{{URL_BASE}}/winthor/integracao/fulfillment/v1/lote",
							"query": [
								{
									"key": "pageSize",
									"value": "{{PAGE_SIZE}}"
								},
								{
									"key": "page",
									"value": "{{PAGE}}"
								}
							]
						},
						"bodyraw": ""
					},
					"response": []
				}
			},
			{
				"nome": "LAYOUTTRANSFORMACAO",
				"valor": [
					{
						"operation": "modify-overwrite-beta",
						"spec": {
							"items": {
								"*": {
									"idExterno": "=concat('wta-get-status-lote-', @(1,numeroLote))"
								}
							}
						}
					},
					{
						"operation": "shift",
						"spec": {
							"items": {
								"*": {
									"numeroLote": "items[&1].numeroLote",
									"idExterno": "idExterno"
								}
							}
						}
					},
					{
						"operation": "default",
						"spec": {
							"_attr_access": "items"
						}
					}
				]
			},
			{
				"nome": "ATIVO",
				"valor": "S"
			},
			{
				"nome": "AUTENTICADOR",
				"valor": "N"
			},
			{
				"nome": "DATASINCRONISMO",
				"valor": "13-NOV-23"
			},
			{
				"nome": "REFRESHTOKEN",
				"valor": "N"
			},
			{
				"nome": "SOMENTEATUALIZARINTEGRACAOCORE",
				"valor": "S"
			}
		]
	}
}
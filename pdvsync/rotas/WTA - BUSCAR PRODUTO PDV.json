										"codigoSubCategoria": "items.[&2].id_retaguarda_sub_categoria",
										"codigoMarca": "items.[&2].id_retaguarda_marca",
										"quantidadeMinimaAtacado": {
											"valor": "items.[&3].quantidadeValorAtacado"
										},
										"embalagens": {

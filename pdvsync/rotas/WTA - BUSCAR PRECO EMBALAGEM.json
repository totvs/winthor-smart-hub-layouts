{
	"tabela": {
		"nome": "PCINTEGRACAOROTASERVICO",
		"campos": [
			{
				"nome": "SOMENTEATUALIZARINTEGRACAOCORE",
				"valor": "N"
			},
			{
				"nome": "ID",
				"valor": "WTA - Buscar Preco Embalagem"
			},
			{
				"nome": "IDEMPRESAAPI",
				"valor": "WINTHOR-WTA"
			},
			{
				"nome": "SERVICO",
				"valor": "WTA - Buscar Preco Embalagem"
			},
			{
				"nome": "LAYOUTCOMUNICACAO",
				"valor": {
					"name": "WTA - Buscar Preco Embalagem",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{TOKEN}}"
							},
							{
								"key": "Accept",
								"value": "*/*"
							}
						],
						"url": {
							"raw": "{{URL_BASE}}/winthor/precos/v1/precos-embalagens",
							"query": [
								{
									"key": "pageSize",
									"value": "{{PAGE_SIZE}}"
								},
								{
									"key": "page",
									"value": "{{PAGE}}"
								},
								{
									"key": "branchId",
									"value": "{{BRANCH_ID}}"
								},
								{
									"key": "lastChange",
									"value": "{{LAST_CHANGE}}"
								},
								{
									"key": "saleType",
									"value": "{{SALE_TYPE}}"
								},
								{
									"key": "merchanType",
									"value": "{{MERCHAN_TYPE}}"
								}
							]
						}
					},
					"response": []
				}
			},
			{
				"nome": "LAYOUTTRANSFORMACAO",
				"valor": [
				  {
					"operation": "modify-overwrite-beta",
					"spec": {
					  "items": {
						"*": {
						  "idExterno": "=concat('pdvsync-preco-embalagem-', @(1,barCodeId),'-',@(1,productId),'-',@(1,branchId),'-',@(1,lastChangeDate))",
						  "idInterno": "=concat(@(1,barCodeId),'-', @(1,productId),'-2017-',@(1,branchId))",
						  "tipoIdInterno": "PDVSYNC-PRECO-EMBALAGEM",
						  "idRetaguarda": "=concat(@(1,barCodeId),'-',@(1,productId),'-2017-',@(1,branchId))",
						  "codigoProduto": "=toString(@(1,productId))",
						  "dataHoraVigenciaInicial": "@(1,startDate)",
						  "dataHoraVigenciaFinal": "@(1,endDate)",
						  "idRetaguardaProduto": "=concat(@(1,barCodeId),'-',@(1,productId))"
						}
					  }
					}
					},
				  {
					"operation": "shift",
					"spec": {
					  "items": {
						"*": {
						  "@": "items[]",
						  "active": {
							"false": {
							  "#0": "items[&3].situacao"
							},
							"*": {
							  "#1": "items[&3].situacao"
							}
						  }
						}
					  }
					}
					},
				  {
					"operation": "shift",
					"spec": {
					  "items": {
						"*": {
						  "idExterno": "items[&1].&",
						  "idInterno": "items[&1].&",
						  "tipoIdInterno": "items[&1].&",
						  "codigoProduto": "items[&1].&",
						  "dataHoraVigenciaInicial": "items[&1].&",
						  "dataHoraVigenciaFinal": "items[&1].&",
						  "idRetaguarda": "items[&1].&",
						  "idRetaguardaProduto": "items[&1].&",
						  "branchId": "items[&1].idProprietario",
						  "situacao": "items[&1].&",
						  "salePrice": "items[&1].valor",
						  "wholesalePrice": "items[&1].ValorAtacado"
						}
					  }
					}
					},
				  {
					"operation": "shift",
					"spec": {
					  "items": {
						"*": {
						  "@": "items[]",
						  "idExterno": "idExterno[]",
						  "idInterno": "idInterno[]",
						  "tipoIdInterno": "tipoIdInterno[]"
						}
					  }
					}
					},
				  {
					"operation": "modify-overwrite-beta",
					"spec": {
					  "items": {
						"*": {
						  "idInquilino": "{{ID_INQUILINO}}",
						  "loteOrigem": "{{LOTE_ORIGEM}}",
						  "PrecoPrincipal": true,
						  "PrecoOrigem": "2017",
						  "IndicePreco": "1"
						}
					  }
					}
					},
				  {
					"operation": "default",
					"spec": {
					  "_attr_access": "items"
					}
					}
				]
			},
			{
				"nome": "ATIVO",
				"valor": "S"
			},
			{
				"nome": "AUTENTICADOR",
				"valor": "N"
			},
			{
				"nome": "DATASINCRONISMO",
				"valor": "14-NOV-23"
			},
			{
				"nome": "REFRESHTOKEN",
				"valor": ""
			},
			{
				"nome": "TIPOPROCESSO",
				"valor": "BUSCAR"
			}
		]
	}
}